grpc:
  enabled: false
  addr: 0.0.0.0
  port: 3001 # we can change to 7776
  tls: false
logging:
  enabled: true
  path: /var/log/mecha-agent.log
  level: info
backend:
  service: 
  messaging: 
data:
  dir: ~/.mecha
status:
  enabled: true
  interval: 60
networking:
  enabled: true
  discovery:
    addr: 0.0.0.0
    port: 7774
  wireguard:
    tun: mecha0
    port: 7775
    userspace: linux
    dns:
      enabled: true
      port: 53
telemetry:
  enabled: true
